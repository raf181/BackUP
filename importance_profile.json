{
  "tiers": [
    {"name": "Documents", "priority": 100, "patterns": [
      "*.pdf", "*.doc", "*.docx", "*.odt", "*.rtf", "*.txt", "*.md",
      "*.xls", "*.xlsx", "*.ods", "*.csv", "*.tsv", "*.ppt", "*.pptx"
    ]},
    {"name": "Project Files", "priority": 95, "patterns": [
      "*.tex", "*.ipynb", "*.py", "*.R", "*.m", "*.java", "*.cs", "*.cpp", "*.c", "*.ts", "*.js"
    ]},
    {"name": "Images", "priority": 90, "patterns": [
      "*.jpg", "*.jpeg", "*.png", "*.gif", "*.tiff", "*.bmp", "*.heic", "*.webp"
    ]},
    {"name": "Audio", "priority": 60, "patterns": [
      "*.mp3", "*.m4a", "*.flac", "*.wav", "*.aac", "*.ogg"
    ]},
    {"name": "Videos", "priority": 50, "patterns": [
      "*.mp4", "*.mov", "*.avi", "*.mkv", "*.webm"
    ]},
    {"name": "Archives", "priority": 40, "patterns": [
      "*.zip", "*.tar", "*.gz", "*.bz2", "*.xz", "*.7z", "*.rar"
    ]},
    {"name": "Everything else", "priority": 10, "patterns": ["*"]}
  ]
}
